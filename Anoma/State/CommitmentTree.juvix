module Anoma.State.CommitmentTree;

import Stdlib.Prelude open;

--- A state root of the commitment tree.
type Root :=
  mkRoot@{
    unRoot : Nat;
  };

open Root using {mkRoot} public;

open Root public;

--- A path in the commitment tree.
type Path :=
  mkPath@{
    unPath : List Nat;
  };

open Path using {mkPath} public;

open Path public;

deriving instance
Root-Ord : Ord Root;

deriving instance
Root-Eq : Eq Root;
